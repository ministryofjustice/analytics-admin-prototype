{
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "moj": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "for-direction": 2,
    "semi": 2,
    "radix": [2, "always"],
    "no-undef": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 1,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2
  }
}
