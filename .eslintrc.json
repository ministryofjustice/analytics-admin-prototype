{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true,
    "moj": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "for-direction": 2,
    "no-undef": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 1,
    "no-console": 1,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 1,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-useless-concat": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "radix": [2, "always"],
    "vars-on-top": 1,
    "wrap-iife": 2,
    "strict": [2, "global"],
    "no-use-before-define": 2,

    "array-bracket-spacing": 2,
    "block-spacing": [2, "never"],
    "brace-style": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": [2, "self"],
    "eol-last": 2,
    "func-call-spacing": 2,
    "indent": [1, 2],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "no-bitwise": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-multi-assign": 2,
    "no-multiple-empty-lines": [2, { "max": 2, "maxEOF": 1 }],
    "no-negated-condition": 1,
    "no-nested-ternary": 2,
    "no-tabs": 2,
    "no-trailing-spaces": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": 2,
    "one-var-declaration-per-line": [2, "always"],
    "semi": 2,
    "semi-style": 2,
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "switch-colon-spacing": 2,
    "wrap-regex": 2
  }
}
